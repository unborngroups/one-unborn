ALTER TABLE client_links
ADD deliverable_id BIGINT UNSIGNED NULL AFTER id,
ADD INDEX client_links_deliverable_id_index (deliverable_id);


ALTER TABLE deliverables
DROP COLUMN dhcp_ip_address,
DROP COLUMN dhcp_vlan,
DROP COLUMN static_ip_address,
DROP COLUMN static_vlan,
DROP COLUMN static_subnet_mask,
DROP COLUMN static_gateway,
DROP COLUMN static_vlan_tag,
DROP COLUMN payment_login_url,
DROP COLUMN payment_quick_url,
DROP COLUMN payment_account_or_username,
DROP COLUMN payment_password,
DROP COLUMN mtu,
DROP COLUMN wifi_username,
DROP COLUMN wifi_password,
DROP COLUMN router_username,
DROP COLUMN router_password;

